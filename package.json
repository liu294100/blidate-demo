{
  "name": "blinddate",
  "private": true,
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "lint": "turbo lint",
    "typecheck": "turbo typecheck",
    "postinstall": "turbo run generate",
    "db:generate": "pnpm --filter @blinddate/database generate",
    "db:push": "pnpm --filter @blinddate/database push",
    "db:seed": "pnpm --filter @blinddate/database seed",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.35",
    "prettier": "^3.2.0",
    "turbo": "^2.3.0",
    "typescript": "^5.5.0"
  },
  "packageManager": "pnpm@9.15.0",
  "engines": {
    "node": ">=20"
  }
}
